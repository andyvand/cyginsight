cp /p/paradyn/release6.1/x86_64-unknown-linux2.4/libelf.so.1 dyninstAPI/$PLAT/lib
cp /p/paradyn/release6.1/x86_64-unknown-linux2.4/libdwarf.so dyninstAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar stackwalkAPI/doc stackwalkAPI/include stackwalkAPI/README stackwalkAPI/LICENSE stackwalkAPI/$PLAT
