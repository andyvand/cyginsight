#!/bin/bash

BUILDDIR=/p/paradyn/development/legendre/release

cd $BUILDDIR
rm -rf builds
rm -rf rel61
mkdir builds
mkdir rel61
cd builds

mkdir dyninstAPI
mkdir symtabAPI
mkdir instructionAPI
mkdir stackwalkAPI

#######################################################
# DyninstAPI
#######################################################

DYNSRC_RM='src/dyninst/codeCoverage src/dyninst/dyninstAPI/tests'

DYNBIN_RM='bin/testprogs bin/testprogs_new include lib/*mrnet* lib/*paradyn* lib/*visi* lib/*pdthread* lib/*pdutil* bin/*barchar* bin/*dbghelp* bin/*igen* bin/*mrnet* bin/*paradyn* bin/*phaseTable* bin/*rthist* bin/*tableVisi* bin/*tclVisi* bin/*topgen* bin/*terrain* bin/*termWin* bin/*barChart* bin/ lib/*testSuite* lib/*xplat* lib/*libtestSuite*'

#
# Build Dyninst Release
#

#Dyninst Source
OFILE=dyninst_src_6.1
echo BUILDING $OFILE
mkdir dyninstAPI/src
cd dyninstAPI/src
cp -r /p/paradyn/builds/dyninst .
cp -r /p/paradyn/builds/scripts .
cp -r /p/paradyn/builds/packagers .
cp -r /p/paradyn/builds/docs .
find . -name .git | xargs rm -rf
cd ../..
cp /p/paradyn/release6.1/LICENSE dyninstAPI/LICENSE
cp /p/paradyn/release6.1/README_dyninst dyninstAPI/README
cd dyninstAPI
rm -rf $DYNSRC_RM
cd ..

tar -cf $OFILE.tar dyninstAPI
gzip $OFILE.tar

#Documentation
mkdir dyninstAPI/doc
cp /p/paradyn/release6.1/doc/dyninstProgGuide.v61.pdf dyninstAPI/doc
cp /p/paradyn/release6.1/doc/instructionProgGuide.pdf dyninstAPI/doc
cp /p/paradyn/release6.1/doc/stackwalkProgGuide.pdf dyninstAPI/doc
cp /p/paradyn/release6.1/doc/symtabProgGuide.pdf dyninstAPI/doc

BINDIR=/p/paradyn/builds

#Includes
cp -r $BINDIR/include dyninstAPI

#Windows
PLAT=i386-unknown-nt4.0
OFILE=dyninst_win_x86_6.1

echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT dyninstAPI/
cp /p/paradyn/packages/WinDebug/dbghelp.dll dyninstAPI/$PLAT/lib
cp /p/paradyn/packages/libxml2/win32/lib/libxml2.dll dyninstAPI/$PLAT/lib
cd dyninstAPI/$PLAT
rm -rf $DYNBIN_RM
cd ../..

echo TAR GZ
tar -cf $OFILE.tar dyninstAPI/doc dyninstAPI/include dyninstAPI/README dyninstAPI/$PLAT
gzip $OFILE.tar

#AIX
PLAT=rs6000-ibm-aix5.1
OFILE=dyninst_aix_power_6.1

echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT dyninstAPI/
cp /p/paradyn/packages/libxml2/rs_aix51/lib/libxml2.so.2 dyninstAPI/$PLAT/lib/libxml2.so
cd dyninstAPI/$PLAT
rm -rf $DYNBIN_RM
cd ../..
chmod 755 dyninstAPI/$PLAT/lib/libDyninstText.a

echo TAR GZ
tar -cf $OFILE.tar dyninstAPI/doc dyninstAPI/include dyninstAPI/README dyninstAPI/$PLAT
gzip $OFILE.tar

#Linux/x86
PLAT=i386-unknown-linux2.4
OFILE=dyninst_linux_x86_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT dyninstAPI/
cd dyninstAPI/$PLAT
rm -rf $DYNBIN_RM
cd ../..
cp /p/paradyn/packages/libxml2/i386_cent40/lib/libxml2.so dyninstAPI/$PLAT/lib
cp /p/paradyn/release6.1/i386-unknown-linux2.4/libelf.so.1 dyninstAPI/$PLAT/lib
cp /p/paradyn/release6.1/i386-unknown-linux2.4/libdwarf.so dyninstAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar dyninstAPI/doc dyninstAPI/include dyninstAPI/README dyninstAPI/$PLAT
gzip $OFILE.tar

#Linux/ia64
PLAT=ia64-unknown-linux2.4
OFILE=dyninst_linux_ia64_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT dyninstAPI/
cd dyninstAPI/$PLAT
rm -rf $DYNBIN_RM
cd ../..
cp /p/paradyn/packages/libxml2/ia64_linux26/lib/libxml2.so dyninstAPI/$PLAT/lib
cp /p/paradyn/release6.1/ia64-unknown-linux2.4/libelf.so.0 dyninstAPI/$PLAT/lib
cp /p/paradyn/release6.1/ia64-unknown-linux2.4/libdwarf.so dyninstAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar dyninstAPI/doc dyninstAPI/include dyninstAPI/README dyninstAPI/$PLAT
gzip $OFILE.tar

#Linux/AMD64
PLAT=x86_64-unknown-linux2.4
OFILE=dyninst_linux_amd64_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT dyninstAPI/
cd dyninstAPI/$PLAT
rm -rf $DYNBIN_RM
cd ../..
cp /p/paradyn/packages/libxml2/amd64_cent40/lib/libxml2.so dyninstAPI/$PLAT/lib
cp /p/paradyn/release6.1/x86_64-unknown-linux2.4/libelf.so.1 dyninstAPI/$PLAT/lib
cp /p/paradyn/release6.1/x86_64-unknown-linux2.4/libdwarf.so dyninstAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar dyninstAPI/doc dyninstAPI/include dyninstAPI/README dyninstAPI/$PLAT
gzip $OFILE.tar

#Linux/PPC32
PLAT=ppc32_linux
OFILE=dyninst_linux_ppc32_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT dyninstAPI/
cd dyninstAPI/$PLAT
rm -rf $DYNBIN_RM
cd ../..
cp /p/paradyn/release6.1/ppc32_linux/libelf.so.0 dyninstAPI/$PLAT/lib
cp /p/paradyn/release6.1/ppc32_linux/libdwarf.so dyninstAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar dyninstAPI/doc dyninstAPI/include dyninstAPI/README dyninstAPI/$PLAT
gzip $OFILE.tar

#AIX/PPC64
PLAT=rs6000-ibm-aix64-5.2
OFILE=dyninst_aix_ppc64_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT dyninstAPI/
cd dyninstAPI/$PLAT
rm -rf $DYNBIN_RM
cd ../..
chmod 755 dyninstAPI/$PLAT/lib/libDyninstText.a

echo TAR GZ
tar -cf $OFILE.tar dyninstAPI/doc dyninstAPI/include dyninstAPI/README dyninstAPI/$PLAT
gzip $OFILE.tar

#Linux/PPC64
PLAT=ppc64_linux
OFILE=dyninst_linux_ppc64_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT dyninstAPI/
cd dyninstAPI/$PLAT
rm -rf $DYNBIN_RM
cd ../..
cp /p/paradyn/release6.1/linux_ppc64/libelf.so.1 dyninstAPI/$PLAT/lib
cp /p/paradyn/release6.1/linux_ppc64/libdwarf.so dyninstAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar dyninstAPI/doc dyninstAPI/include dyninstAPI/README dyninstAPI/$PLAT
gzip $OFILE.tar

#Solaris/SPARC
PLAT=sparc-sun-solaris2.9
OFILE=dyninst_solaris_sparc_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT dyninstAPI/
cd dyninstAPI/$PLAT
rm -rf $DYNBIN_RM
cd ../..
cp /p/paradyn/release6.1/sparc-sun-solaris2.9/libdwarf.so dyninstAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar dyninstAPI/doc dyninstAPI/include dyninstAPI/README dyninstAPI/$PLAT
gzip $OFILE.tar

#MOVE GZs
mv *.gz ../rel61

#######################################################
# SymtabAPI
#######################################################

SYMSRC_RM='src/dyninst/codeCoverage src/dyninst/dyner src/dyninst/dyninstAPI src/dyninst/dyninstAPI_RT src/dyninst/parseThat src/dyninst/sharedMem src/dyninst/testsuite src/dyninst/DyninstAPI* src/dyninst/Paradyn.sln src/dyninst/depGraphAPI src/dyninst/instructionAPI src/dyninst/stackwalk src/dyninst/valueAdded src/testsuite/src/instruction src/testsuite/src/dyninst'

SYMBIN_RM='bin/testprogs_new lib/*dyninstAPI* lib/*ShmMut* lib/*testsuite* lib/*sharedMut* lib/*testSuite* lib/*instructionAPI* lib/*stackwalk* lib/*DyninstText*'

#
# Build Symtab Release
#

#Symtab Source
OFILE=symtab_src_6.1
echo BUILDING $OFILE
mkdir symtabAPI/src
cd symtabAPI/src
cp -r /p/paradyn/builds/dyninst .
cp -r /p/paradyn/builds/scripts .
cp -r /p/paradyn/builds/packagers .
cd ../..
cp /p/paradyn/release6.1/LICENSE symtabAPI/LICENSE
cp /p/paradyn/release6.1/README_symtab symtabAPI/README
cp /p/paradyn/release6.1/Makefile.symtab symtabAPI/src/dyninst/Makefile
cd symtabAPI
rm -rf $DYNSRC_RM 
rm -rf $SYMSRC_RM
cd ..

tar -cf $OFILE.tar symtabAPI
gzip $OFILE.tar

#Documentation
mkdir symtabAPI/doc
cp /p/paradyn/release6.1/doc/symtabProgGuide.pdf symtabAPI/doc

BINDIR=/p/paradyn/builds

#Includes
mkdir symtabAPI/include
cp $BINDIR/dyninst/symtabAPI/h/*.h symtabAPI/include
cp $BINDIR/dyninst/dynutil/h/*.h symtabAPI/include
cp -r $BINDIR/dyninst/dynutil/h/dyn_detail symtabAPI/include

#Windows
PLAT=i386-unknown-nt4.0
OFILE=symtab_win_x86_6.1

echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT symtabAPI/
cp /p/paradyn/packages/WinDebug/dbghelp.dll symtabAPI/$PLAT/lib
cp /p/paradyn/packages/libxml2/win32/lib/libxml2.dll symtabAPI/$PLAT/lib
cd symtabAPI/$PLAT
rm -rf $DYNBIN_RM
rm -rf $SYMBIN_RM
cd ../..

echo TAR GZ
tar -cf $OFILE.tar symtabAPI/doc symtabAPI/include symtabAPI/README symtabAPI/LICENSE symtabAPI/$PLAT
gzip $OFILE.tar

#AIX
PLAT=rs6000-ibm-aix5.1
OFILE=symtab_aix_power_6.1

echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT symtabAPI/
cp /p/paradyn/packages/libxml2/rs_aix51/lib/libxml2.so.2 symtabAPI/$PLAT/lib/libxml2.so
cd symtabAPI/$PLAT
rm -rf $DYNBIN_RM
rm -rf $SYMBIN_RM
cd ../..

echo TAR GZ
tar -cf $OFILE.tar symtabAPI/doc symtabAPI/include symtabAPI/README symtabAPI/LICENSE symtabAPI/$PLAT
gzip $OFILE.tar

#Linux/x86
PLAT=i386-unknown-linux2.4
OFILE=symtab_linux_x86_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT symtabAPI/
cd symtabAPI/$PLAT
rm -rf $DYNBIN_RM
rm -rf $SYMBIN_RM
cd ../..
cp /p/paradyn/packages/libxml2/i386_cent40/lib/libxml2.so symtabAPI/$PLAT/lib
cp /p/paradyn/release6.1/i386-unknown-linux2.4/libelf.so.1 symtabAPI/$PLAT/lib
cp /p/paradyn/release6.1/i386-unknown-linux2.4/libdwarf.so symtabAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar symtabAPI/doc symtabAPI/include symtabAPI/README symtabAPI/LICENSE symtabAPI/$PLAT
gzip $OFILE.tar

#Linux/ia64
PLAT=ia64-unknown-linux2.4
OFILE=symtab_linux_ia64_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT symtabAPI/
cd symtabAPI/$PLAT
rm -rf $DYNBIN_RM
rm -rf $SYMBIN_RM
cd ../..
cp /p/paradyn/packages/libxml2/ia64_linux26/lib/libxml2.so symtabAPI/$PLAT/lib
cp /p/paradyn/release6.1/ia64-unknown-linux2.4/libelf.so.0 symtabAPI/$PLAT/lib
cp /p/paradyn/release6.1/ia64-unknown-linux2.4/libdwarf.so symtabAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar symtabAPI/doc symtabAPI/include symtabAPI/README symtabAPI/LICENSE symtabAPI/$PLAT
gzip $OFILE.tar

#Linux/AMD64
PLAT=x86_64-unknown-linux2.4
OFILE=symtab_linux_amd64_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT symtabAPI/
cd symtabAPI/$PLAT
rm -rf $DYNBIN_RM
rm -rf $SYMBIN_RM
cd ../..
cp /p/paradyn/packages/libxml2/amd64_cent40/lib/libxml2.so symtabAPI/$PLAT/lib
cp /p/paradyn/release6.1/x86_64-unknown-linux2.4/libelf.so.1 symtabAPI/$PLAT/lib
cp /p/paradyn/release6.1/x86_64-unknown-linux2.4/libdwarf.so symtabAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar symtabAPI/doc symtabAPI/include symtabAPI/README symtabAPI/LICENSE symtabAPI/$PLAT
gzip $OFILE.tar

#Linux/PPC32
PLAT=ppc32_linux
OFILE=symtab_linux_ppc32_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT symtabAPI/
cd symtabAPI/$PLAT
rm -rf $DYNBIN_RM
rm -rf $SYMBIN_RM
cd ../..
cp /p/paradyn/release6.1/ppc32_linux/libelf.so.0 symtabAPI/$PLAT/lib
cp /p/paradyn/release6.1/ppc32_linux/libdwarf.so symtabAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar symtabAPI/doc symtabAPI/include symtabAPI/README symtabAPI/LICENSE symtabAPI/$PLAT
gzip $OFILE.tar

#Linux/PPC64
PLAT=ppc64_linux
OFILE=symtab_linux_ppc64_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT symtabAPI/
cd symtabAPI/$PLAT
rm -rf $DYNBIN_RM
rm -rf $SYMBIN_RM
cd ../..
cp /p/paradyn/release6.1/linux_ppc64/libelf.so.1 symtabAPI/$PLAT/lib
cp /p/paradyn/release6.1/linux_ppc64/libdwarf.so symtabAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar symtabAPI/doc symtabAPI/include symtabAPI/README symtabAPI/LICENSE symtabAPI/$PLAT
gzip $OFILE.tar

#Solaris/SPARC
PLAT=sparc-sun-solaris2.9
OFILE=symtab_solaris_sparc_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT symtabAPI/
cd symtabAPI/$PLAT
