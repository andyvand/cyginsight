#Linux/PPC32
PLAT=ppc32_linux
OFILE=stackwalk_linux_ppc32_1.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT stackwalkAPI/
cd stackwalkAPI/$PLAT
rm -rf $DYNBIN_RM
rm -rf $SWKBIN_RM
cd ../..
cp /p/paradyn/release6.1/ppc32_linux/libelf.so.0 stackwalkAPI/$PLAT/lib
