rm -rf $SYMBIN_RM
cd ../..
cp /p/paradyn/release6.1/sparc-sun-solaris2.9/libdwarf.so symtabAPI/$PLAT/lib

echo TAR GZ
tar -cf $OFILE.tar symtabAPI/doc symtabAPI/include symtabAPI/README symtabAPI/LICENSE symtabAPI/$PLAT
gzip $OFILE.tar

#AIX/PPC64
PLAT=rs6000-ibm-aix64-5.2
OFILE=symtab_aix_ppc64_6.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT symtabAPI/
cd symtabAPI/$PLAT
rm -rf $DYNBIN_RM
rm -rf $SYMBIN_RM
cd ../..

echo TAR GZ
tar -cf $OFILE.tar symtabAPI/doc symtabAPI/include symtabAPI/README symtabAPI/LICENSE symtabAPI/$PLAT
gzip $OFILE.tar


#MOVE GZs
mv *.gz ../rel61

#######################################################
# InstructionAPI
#######################################################

INSSRC_RM='src/dyninst/codeCoverage src/dyninst/dyner src/dyninst/dyninstAPI src/dyninst/dyninstAPI_RT src/dyninst/parseThat src/dyninst/sharedMem src/dyninst/testsuite src/dyninst/DyninstAPI* src/dyninst/Paradyn.sln src/dyninst/symtabAPI src/dyninst/depGraphAPI src/dyninst/stackwalk src/dyninst/valueAdded src/dyninst/testsuite/src/dyninst src/dyninst/testsuite/symtab'

INSBIN_RM='bin/testprogs_new lib/*dyninstAPI* lib/*ShmMut* lib/*testsuite* lib/*sharedMut* lib/*testSuite* lib/*symtabAPI* lib/*stackwalk*'

#
# Build InstructionAPI Release
#

#InstructionAPI Source
OFILE=instruction_src_1.1
echo BUILDING $OFILE
mkdir instructionAPI/src
cd instructionAPI/src
cp -r /p/paradyn/builds/dyninst .
cp -r /p/paradyn/builds/scripts .
cd ../..
cp /p/paradyn/release6.1/LICENSE instructionAPI/LICENSE
cp /p/paradyn/release6.1/README_instruction instructionAPI/README
cp /p/paradyn/release6.1/Makefile.instruction instructionAPI/src/dyninst/Makefile
cd instructionAPI
rm -rf $DYNSRC_RM 
rm -rf $INSSRC_RM
cd ..

tar -cf $OFILE.tar instructionAPI
gzip $OFILE.tar

#Documentation
mkdir instructionAPI/doc
cp /p/paradyn/release6.1/doc/instructionProgGuide.pdf instructionAPI/doc

BINDIR=/p/paradyn/builds

#Includes
mkdir instructionAPI/include
cp $BINDIR/dyninst/instructionAPI/h/*.h instructionAPI/include
cp $BINDIR/dyninst/dynutil/h/*.h instructionAPI/include
cp -r $BINDIR/dyninst/dynutil/h/dyn_detail instructionAPI/include

#Linux/x86
PLAT=i386-unknown-linux2.4
OFILE=instruction_linux_x86_1.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT instructionAPI/
cd instructionAPI/$PLAT
rm -rf $DYNBIN_RM
rm -rf $INSBIN_RM
cd ../..

echo TAR GZ
tar -cf $OFILE.tar instructionAPI/doc instructionAPI/include instructionAPI/README instructionAPI/LICENSE instructionAPI/$PLAT
gzip $OFILE.tar

#Linux/AMD64
PLAT=x86_64-unknown-linux2.4
OFILE=instruction_linux_amd64_1.1
echo COPYING FILES FOR $PLAT

cp -r $BINDIR/$PLAT instructionAPI/
cd instructionAPI/$PLAT
rm -rf $DYNBIN_RM
rm -rf $INSBIN_RM
cd ../..

echo TAR GZ
tar -cf $OFILE.tar instructionAPI/doc instructionAPI/include instructionAPI/README instructionAPI/LICENSE instructionAPI/$PLAT
gzip $OFILE.tar

#MOVE GZs
mv *.gz ../rel61

#######################################################
# StackwalkerAPI
#######################################################

SWKSRC_RM='src/dyninst/codeCoverage src/dyninst/dyner src/dyninst/dyninstAPI src/dyninst/dyninstAPI_RT src/dyninst/parseThat src/dyninst/sharedMem src/dyninst/testsuite src/dyninst/DyninstAPI* src/dyninst/Paradyn.sln src/dyninst/depGraphAPI src/dyninst/valueAdded src/dyninst/testsuite src/dyninst/instructionAPI'

